<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.interpreter.interpreter" >

    <permission android:name="com.android.interpreter.C2D_MESSAGE" android:protectionLevel="signature" />
    <uses-permission android:name="ccom.android.interpreter.permission.C2D_MESSAGE" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.SEND" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".ConversationsActivity"
            android:label="@string/conversations_activity" >
        </activity>

        <activity
            android:name=".ConfigurationActivity"
            android:label="@string/title_activity_configuration" >
        </activity>

        <activity
            android:name=".MessageDetailsActivity"
            android:label="@string/title_activity_message_details" >
        </activity>

        <activity
            android:name=".ConfigurationMessageActivity"
            android:label="@string/title_activity_configuration_message" >
        </activity>

        <activity
            android:name=".ChatActivity"
            android:label="@string/title_activity_chat" >
        </activity>


    </application>

</manifest>
